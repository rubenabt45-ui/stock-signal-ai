
# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe Configuration
# Set to 'live' for production, 'test' for development
VITE_STRIPE_MODE=test

# Test Keys (for development)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_test_key

# Live Keys (for production - uncomment and use when VITE_STRIPE_MODE=live)
# VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_live_key

# Feature Flags
# Set to false in production to disable test utilities
VITE_ENABLE_STRIPE_TEST=false

# Analytics Configuration (optional)
# For Google Analytics, use GA measurement ID (G-XXXXXXXXXX)
# For Plausible, use your domain
VITE_ANALYTICS_ID=
VITE_ANALYTICS_PROVIDER=gtag

# Development Settings
# Set to true only in development
VITE_DEV_MODE=false

# Application Settings
VITE_APP_NAME=TradeIQ
VITE_APP_DESCRIPTION=AI-Powered Trading Platform
